<%- include('partials/header')-%>

<h1>Home</h1>

<p><%=startingContent%></p>

<%posts.forEach(function(post){%>
  <% let intededTitle=post.blogTitle[0].toUpperCase()+post.blogTitle.slice(1,post.blogTitle.length)%>
  <h1><%=intededTitle%></h1>
  <p><%=post.blogBody.slice(0,150)+" ...."%><a href="/posts/<%=posts.title%>">Read More</a></p>
<%});%>


<%-include('partials/footer')-%>
